#include <stdio.h>

/**
 * Main - Pritn Fizz or Buzz depending multiples 3 or 5
 *
 * void: No input variables
 * Return: No return
 */

int main(void)
{
	int cont;

	for (cont = 1; cont <= 100; cont++)
	{
		if (cont % 3 == 0 && cont % 5 == 0)
			printf("FizzBuzz ");
		else
		{
			if (cont % 3 == 0)
			printf("Fizz ");
			if (cont % 5 == 0)
			printf("Buzz ");
		}
		if (cont % 3 != 0 && cont % 5 != 0)
			printf("%d ", cont);
	}
	printf("\n");
	return (0);
}
