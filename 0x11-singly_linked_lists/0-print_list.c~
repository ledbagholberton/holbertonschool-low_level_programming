#include "lists.h"

/**
 * print_list - print all the list linked
 *
 * Return: Type size_t return number of of nodes in the list.
 */

size_t print_list(const list_t *h)
{
	size_t a;
	a = 0;

	while (h != NULL)
	{
		if ((*h).str == NULL)
			(*h).str = 0;
		printf("[%d] %s\n", (*h).len, (*h).str);
		a++;
		h = (*h).next;
	}
	return(a);
}

