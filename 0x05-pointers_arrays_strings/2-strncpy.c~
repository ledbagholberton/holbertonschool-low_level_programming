#include "holberton.h"
/**
 * reset_to_98 - change the value of n to 98
 *
 * @n: Address of n
 * Return: Pointer n value changed to 98
 */


char *_strncpy(char *dest, char *src, int n)
{
	int i;
	
	for (i = 0; i < n && src[i] != '\0'; i++)
		dest[i] = src[i];
	while (i < n)
	{
		dest[i] = '\0';
		i++;
	}
	return (dest);
 }
