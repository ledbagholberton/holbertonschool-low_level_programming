#include "3-calc.h"

/**
 * op_add - add two numbers
 * @a: Number a
 * @b: Number b
 *
 * Return: Add a + b
 */

int op_add(int a, int b);
{
	return (a + b);
}

/**
 * op_sub - substract two numbers
 * @a: Number a
 * @b: Number b
 *
 * Return: Substraction a - b
 */

int op_sub(int a, int b);
{
	return (a - b);
}

/**
 * op_mul - a times b
 * @a: Number a
 * @b: Number b
 *
 * Return: Multiplication a * b
 */

int op_mul(int a, int b);
{
	return (a * b);
}

/**
 * op_div - divdide two numbers
 * @a: Number a
 * @b: Number b
 *
 * Return: Div a / b
 */

int op_div(int a, int b);
{
	return (a / b);
}

/**
 * op_add - Module two numbers
 * @a: Number a
 * @b: Number b
 *
 * Return: a % b
 */

int op_mod(int a, int b);
{
	return (a % b);
}
